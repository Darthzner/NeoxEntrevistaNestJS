generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  user             String  @id(map: "users_pk") @db.VarChar 
  password         String  @db.VarChar
  secret_code      String  @db.VarChar
  is_activate      Boolean @default(true)
  
}

